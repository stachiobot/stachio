{
	"name": "bot-template",
	"version": "1.0.0",
	"description": "TypeScript discord.js bot template by Project Discord",
	"private": true,
	"workspaces": [
		"packages/*",
		"bots/*",
		"tools/*"
	],
	"homepage": "https://github.com/ProjectDiscord/bot-template#readme",
	"bugs": {
		"url": "https://github.com/ProjectDiscord/bot-template/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ProjectDiscord/bot-template.git"
	},
	"license": "MIT",
	"author": "ProjectDiscord",
	"scripts": {
		"build": "turbo run build --force",
		"dev": "turbo run dev --parallel",
		"lint": "turbo run lint",
		"format": "turbo run format",
		"Split": "________________________________________________________________",
		"start:bot": "node ./bots/client/dist/index.js",
		"start:helper": "node ./bots/helper/dist/index.js",
		"start:processer": "node ./packages/processer/dist/index.js",
		"start:dashboard": "node ./packages/dashboard/dist/index.js",
		"Split_2": "________________________________________________________________",
		"generate:authio": "prisma generate --schema=prisma/authio/schema.prisma",
		"generate:global": "prisma generate --schema=prisma/global/schema.prisma",
		"generate:monitio": "prisma generate --schema=prisma/monitio/schema.prisma",
		"generate:sentio": "prisma generate --schema=prisma/sentio/schema.prisma",
		"generate:stachio": "prisma generate --schema=prisma/stachio/schema.prisma",
		"generate:all": "npm run generate:authio && npm run generate:global && npm run generate:monitio && npm run generate:sentio && npm run generate:stachio",
		"Split_3": "________________________________________________________________",
		"migrate:authio": "prisma migrate deploy --schema=prisma/authio/schema.prisma",
		"migrate:global": "prisma migrate deploy --schema=prisma/global/schema.prisma",
		"migrate:monitio": "prisma migrate deploy --schema=prisma/monitio/schema.prisma",
		"migrate:sentio": "prisma migrate deploy --schema=prisma/sentio/schema.prisma",
		"migrate:stachio": "prisma migrate deploy --schema=prisma/stachio/schema.prisma",
		"migrate:all": "npm run migrate:authio && npm run migrate:global && npm run migrate:monitio && npm run migrate:sentio && npm run migrate:stachio",
		"Split_4": "________________________________________________________________",
		"migrate:authio:dev": "prisma migrate dev --schema=prisma/authio/schema.prisma",
		"migrate:global:dev": "prisma migrate dev --schema=prisma/global/schema.prisma",
		"migrate:monitio:dev": "prisma migrate dev --schema=prisma/monitio/schema.prisma",
		"migrate:sentio:dev": "prisma migrate dev --schema=prisma/sentio/schema.prisma",
		"migrate:stachio:dev": "prisma migrate dev --schema=prisma/stachio/schema.prisma",
		"migrate:dev:all": "npm run migrate:authio:dev && npm run migrate:global:dev && npm run migrate:monitio:dev && npm run migrate:sentio:dev && npm run migrate:stachio:dev",
		"Split_5": "________________________________________________________________",
		"db:reset": "npx prisma migrate reset --force --schema=prisma/stachio/schema.prisma",
		"db:studio": "npx prisma studio --schema=prisma/stachio/schema.prisma",
		"Split_6": "________________________________________________________________",
		"setup:stachio": "npm run migrate:stachio && npm run generate:stachio",
		"setup:all": "npm run migrate:all && npm run generate:all",
		"Split_7": "________________________________________________________________",
		"check:prisma": "npx prisma validate --schema=prisma/stachio/schema.prisma",
		"check:types": "tsc --noEmit"
	},
	"devDependencies": {
		"prettier": "^3.6.2",
		"prisma": "^6.16.2",
		"turbo": "^2.5.6"
	},
	"dependencies": {
		"@prisma/client": "^6.16.2"
	},
	"packageManager": "npm@11.6.0"
}
